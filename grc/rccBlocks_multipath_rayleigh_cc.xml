<?xml version="1.0"?>
<block>
  <name>multipath_rayleigh_cc</name>
  <key>rccBlocks_multipath_rayleigh_cc</key>
  <category>rccBlocks</category>
  <import>import rccBlocks</import>
  <make>rccBlocks.multipath_rayleigh_cc(src, vehicle_speed, carrier_freq, chan_rate, chan_seed, chan_pwrs, path_delays, flag_indep, flag_norm)</make>
  <!-- Make one 'param' node for every Parameter you want settable from the GUI.
       Sub-nodes:
       * name
       * key (makes the value accessible as $keyname, e.g. in the make node)
       * type -->
  <param>
    <name>...</name>
    <key>...</key>
    <type>...</type>
  </param>

  <!-- Make one 'sink' node per input. Sub-nodes:
       * name (an identifier for the GUI)
       * type
       * vlen
       * optional (set to 1 for optional inputs) -->
  <sink>
    <name>in</name>
    <type><!-- e.g. int, real, complex, byte, short, xxx_vector, ...--></type>
  </sink>

  <!-- Make one 'source' node per output. Sub-nodes:
       * name (an identifier for the GUI)
       * type
       * vlen
       * optional (set to 1 for optional inputs) -->
  <source>
    <name>out</name>
    <type><!-- e.g. int, real, complex, byte, short, xxx_vector, ...--></type>
  </source>
</block>
